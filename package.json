{
  "name": "itty-router-extras",
  "version": "0.4.2",
  "description": "An assortment of delicious extras for the calorie-light itty-router.",
  "main": "./index.js",
  "types": "./dist/itty-router-extras.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "lint": "npx eslint src",
    "test": "vitest",
    "verify": "echo 'verifying module...' && yarn build && yarn test",
    "dev": "run-p lint test",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "prerelease": "yarn verify",
    "prebuild": "rimraf dist && mkdir dist && cp src/itty-router-extras.d.ts dist",
    "build": "tsup src/index.ts --format cjs,esm --minify --dts --clean",
    "release": "release --tag --push --src=dist"
  },
  "keywords": [
    "router",
    "cloudflare",
    "workers",
    "serverless",
    "regex",
    "routing",
    "api",
    "handler",
    "params",
    "middleware",
    "nested",
    "helpers",
    "utils",
    "cookies",
    "content",
    "cors"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwhitley/itty-router-extras.git"
  },
  "author": "Kevin R. Whitley <krwhitley@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kwhitley/itty-router-extras/issues"
  },
  "homepage": "https://github.com/kwhitley/itty-router-extras#readme",
  "devDependencies": {
    "@cloudflare/workers-types": "3.17.0",
    "@edge-runtime/vm": "1.1.0-beta.31",
    "@typescript-eslint/parser": "5.40.0",
    "chalk": "^4.1.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-jest": "^24.3.2",
    "fs-extra": "^9.1.0",
    "gzip-size": "^6.0.0",
    "itty-router": "^2.3.2",
    "jest": "^26.6.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "terser": "^5.6.1",
    "terser-folder": "^2.0.0",
    "tsup": "6.2.3",
    "typescript": "4.8.4",
    "vitest": "0.24.3",
    "yarn-release": "^1.10.3"
  }
}
